                    <topic name="ANSWER">
                    <category>
                        <pattern>*</pattern>
                        <template>
                        <think><set name="rsp"><star/></set></think> 
                        <condition name="rsp">
                            <li value="YES"><think><set name="topic"></set></think><link><text><get name="starname"/></text><url><map name="starlist"><get name="starname"/></map></url></link><srai>SHALL WE FIND A STAR</srai></li>
                            <li value="NO"><think><set name="topic"></set></think>Okay, now you cand find another star, choose category.<br/><srai>SHALL WE FIND A STAR</srai></li>    
                            <li>Please type Yes or No.</li>
                        </condition>
                        </template>
